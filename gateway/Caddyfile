(cloudflare) {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
		resolvers 1.1.1.1 1.0.0.1
	}
}

vault.{$DOMAIN} {
	reverse_proxy https://vaultwarden:8000 {
		transport http {
			tls_insecure_skip_verify
		}
	}
	import cloudflare
}

music.{$DOMAIN} {
	reverse_proxy navidrome:4533
	import cloudflare
}

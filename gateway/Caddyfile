(cloudflare) {
	tls {
		dns cloudflare {env.CLOUDFLARE_API_TOKEN}
		resolvers 1.1.1.1 1.0.0.1
	}
}

vault.{$DOMAIN} {
	import cloudflare
	reverse_proxy vaultwarden:80
}

music.{$DOMAIN} {
	import cloudflare
	reverse_proxy navidrome:4533
}

money.{$DOMAIN} {
	import cloudflare
	reverse_proxy ihatemoney:8000
}
